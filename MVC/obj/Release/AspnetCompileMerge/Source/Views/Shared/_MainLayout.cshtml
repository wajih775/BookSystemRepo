<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="initial-scale=1">

    <link href="../../content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="../../content/css/style.css" rel="stylesheet" />
    
    

    <script src="../../content/js/jquery.js"></script>
    <script src="~/content/js/jquery.timeago.js"></script>
    <script src="~/content/js/main.js"></script>
    
    @*Modal Plugin :: http://dimsemenov.com/plugins/magnific-popup/*@
    <link href="../../content/plugins/magnific-popup/css/magnific-popup.css" rel="stylesheet" />
    <script src="../../content/plugins/magnific-popup/js/jquery.magnific-popup.min.js"></script>

   @*Date Picker :: http://eternicode.github.io/bootstrap-datepicker/*@
    <link href="~/content/css/datepicker3.css" rel="stylesheet" />
    <script src="~/content/js/bootstrap-datepicker.js"></script>
    
     @*Form Validation :: http://formvalidation.io/getting-started/*@
    <link href="~/content/plugins/formValidation/css/formValidation.min.css" rel="stylesheet" />
    <script src="~/content/plugins/formValidation/js/formValidation.min.js"></script>
    <script src="~/content/plugins/formValidation/js/fmbootstrap.min.js"></script>


    @RenderSection("head",false)

    <link href="../../content/css/custom.css" rel="stylesheet" />
</head>

<body class=" page custom-background  wp-job-manager-categories-enabled wp-resume-manager-categories-enabled wp-job-manager-contact-listing">

    <div id="page" >

        <header id="masthead" class="site-header" role="banner">
            <div class="container">
                <a href="#" title="Jobs" rel="home" class="site-branding">
                    <h1 class="site-title">
                        <img src="../../content/images/logo.png" width="" height="" alt="" />
                    </h1>
                </a>
                <nav id="site-navigation" class="site-primary-navigation slide-left">
                   <ul id="menu-main-menu" class="nav-menu-primary">@Html.Partial("_LoginPartial")</ul>
                </nav>

                <a href="#" class="primary-menu-toggle in-header"><i class="icon-menu"></i></a>
            </div>
        </header>
        <!-- #masthead -->
        
        <div id="main" class="site-main">

            <div id="primary" >
                <div id="content" role="main">
                    @RenderBody()
                </div>
                <!-- #content -->
            </div>
            <!-- #primary -->


        </div>
        <!-- #main -->

      

       
        <!-- #colophon -->
    </div>
    <!-- #page -->






    <script type='text/javascript'>
        /* <![CDATA[ */
        var recruitSettings = {  "i18n": [], "pages": { "is_widget_home": true, "is_job": false, "is_resume": false, "is_testimonials": false }, "widgets": { "recruit_widget_slider_hero": { "animate": 0 }, "recruit_widget_jobs": { "animate": 0 }, "recruit_widget_stats": { "animate": 1 }, "recruit_widget_companies": { "animate": 0 }, "recruit_widget_testimonials": { "animate": 1 }, "recruit_widget_video": { "animate": 1 }, "recruit_widget_slider": { "animate": 0 }, "recruit_widget_price_table_wc": { "animate": 0 }, "recruit_widget_callout": { "animate": 0 }, "recruit_widget_blog_posts": { "animate": 0 } } };
        /* ]]> */
    </script>
    <script src="../../content/js/jobify.min.js"></script>
    
    @RenderSection("footer",false)

</body>
</html>

